{
  "expo": {
    "name": "LocationTrackingAppFront",
    "slug": "LocationTrackingAppFront",
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow LocationTrackingAppFront to access your photos.",
          "savePhotosPermission": "Allow LocationTrackingAppFront to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "config": {
        "googleMapsApiKey": "AIzaSyD-VNM4AaQkbnJn4_rnpm1COrE2Mf2cRYE",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "App requires geolocation to improve the quality of the service",
        "NSLocationAlwaysUsageDescription": "App requires geolocation to improve the quality of the service",
        "NSLocationWhenInUseUsageDescription": "App requires geolocation to improve the quality of the service",
        "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
        "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
        "isAccessMediaLocationEnabled": true
      },
      "infoPlist": {
        "bundleIdentifier": "com.ourtor.ourtorapp", 
        "buildNumber": "0.0.8", 
        "googleServicesFile": "./GoogleService-Info.plist",
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    }
    ,
    "android":{
      "config":{
          "googleMaps":{
              "apiKey": "AIzaSyD-VNM4AaQkbnJn4_rnpm1COrE2Mf2cRYE"
          }
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "notification": {
      "icon": "./assets/notificationicon.png",
      "color": "#3cb3ff"
    }
  }
}
